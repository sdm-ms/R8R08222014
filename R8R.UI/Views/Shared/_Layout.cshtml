    <script src="@Url.Content("~/Content/js/jquery-1.3.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/cycleinvoke.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.cycle.all.min.js")" type="text/javascript"></script>



    @*<asp:ContentPlaceHolder ID="head1" runat="server">
        </asp:ContentPlaceHolder>*@
